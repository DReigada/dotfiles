{
  "editor.fontFamily": "'Source Code Pro', 'Meslo LG S DZ for Powerline', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "workbench.startupEditor": "newUntitledFile",
  "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
